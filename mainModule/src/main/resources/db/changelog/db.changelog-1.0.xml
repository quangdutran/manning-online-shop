<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="dutq" id="changelog-1.0">
        <createTable tableName="user">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="active" type="bit">
            </column>
            <column name="email" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="first_name" type="varchar(255)"/>
            <column name="last_name" type="varchar(255)"/>
            <column name="date_created" type="datetime"/>
            <column name="date_modified" type="datetime"/>
            <column name="update_id" type="varchar(100)"/>
            <column name="provider_id" type="varchar(50)"/>
            <column name="access_token" type="varchar(50)"/>
            <column name="display_name" type="varchar(50)"/>
            <column name="image_url" type="varchar(100)"/>
        </createTable>
        <createTable tableName="order">
            <column name="id" type="int" autoIncrement="true">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="desc" type="varchar(10)"/>
        </createTable>
        <rollback>
            <dropTable tableName="user"/>
            <dropTable tableName="order"/>
        </rollback>
    </changeSet>

</databaseChangeLog>